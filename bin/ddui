#!/usr/bin/env php
<?php
require __DIR__.'/../vendor/autoload.php';

use Symfony\Component\Console\Application;
use Grasmash\DdUi\Command\GetPageCommand;

$application = new Application('ddui', '1.0.0');
$commands = [
  new GetPageCommand(),
];

$application->addCommands($commands);
$application->run();